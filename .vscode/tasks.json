{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "NeoRust doctest",
        "type": "shell",
        "command": "cargo",
        "args": [
            "test",
            "--doc",
            "--package",
            "NeoRust"
        ],
        "problemMatcher": [],
        "group": {
          "kind": "build",
          "isDefault": true
        }
      },
      {
        "label": "cargo build",
        "type": "shell",
        "command": "cargo",
        "args": ["build"],
        "group": {
          "kind": "build",
          "isDefault": true
        }
      },
      {
        "label": "cargo clean",
        "type": "shell",
        "command": "cargo",
        "args": ["clean"],
        "problemMatcher": [],
        "group": {
          "kind": "build",
          "isDefault": true
        }
      },
    ]
  }